{
  "name": "blogs_app",
  "version": "1.0.0",
  "description": "Web application for viewing personal vlogs",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo",
    "makemigrations": "npx sequelize-cli migration:create --name added_is_verified_column",
    "createmigrations": "npx sequelize-cli model:generate --name Category --attributes title:string",
    "insert": "npx sequelize-cli seed:generate --name demo-user",
    "seed": "npx sequelize-cli db:seed:all",
    "swagger-autogen": "node swagger.js"
  },
  "keywords": [
    "node",
    "nodemon",
    "blogs"
  ],
  "author": "Poojan Pradhan",
  "license": "ISC",
  "dependencies": {
    "adminjs": "5.10.4",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "3.1.1",
    "joi": "^17.6.0",
    "joi-objectid": "^4.0.2",
    "joi-password-complexity": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.19",
    "sequelize-cli": "^6.4.1",
    "swagger-autogen": "2.21.5",
    "swagger-ui-express": "4.5.0"
  }
}
